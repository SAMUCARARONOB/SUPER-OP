local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua'))()
getgenv().SecureMode = true

local Window = Rayfield:CreateWindow({
    Name = "OP SCRIPTS",
    LoadingTitle = "OP SCRIPTS",
    LoadingSubtitle = "Brought to you by SCRIPTS OP",
})

local Tab = Window:CreateTab("Main GUI's")
local Section = Tab:CreateSection("Blade Ball Main GUI's")
local Label = Tab:CreateLabel("Thanks for using OP SCRIPTS☠️")

local scripts = {
    {"Inferno Hub", "https://github.com/SadlekAski/Scripts/raw/main/Blade%20Ball/Equip%20any%20ability.lua"},
